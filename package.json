{
  "name": "promotion-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:server": "http-server ./dist/promotion-project -P http://localhost:4200? -a localhost -p 4200 -c-1",
    "server:up": "ng build --prod && run-p --race start:server",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run ",
    "cy:ci": "ng build --prod && run-p --race start:server cy:run",
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "build:prod": "npm run ng-high-memory -- build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~10.0.9",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@angular/service-worker": "~10.0.9",
    "@ngrx/effects": "^10.1.1",
    "@ngrx/entity": "^10.1.1",
    "@ngrx/router-store": "^10.1.1",
    "@ngrx/store": "^10.1.1",
    "@ngrx/store-devtools": "^10.1.1",
    "@ngx-formly/core": "^5.0.0",
    "@ngx-formly/material": "^5.0.0",
    "@ngx-formly/schematics": "^5.10.11",
    "firebase": "^7.0 || ^8.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/architect": ">= 0.900 < 0.1200",
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@briebug/cypress-schematic": "^4.4.0",
    "@cypress/webpack-preprocessor": "5.5.0",
    "@ngrx/schematics": "^10.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.20.7",
    "codelyzer": "^6.0.1",
    "cypress": "^7.0.0",
    "cypress-file-upload": "^5.0.3",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.15.0",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "husky": "^4.3.6",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.8.1",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.3",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-loader": "8.0.12",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "pretty-quick --staged"
      ],
      "pre-push": [
        "npm run lint"
      ]
    }
  }
}
